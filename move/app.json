{
  "expo": {
    "web": {
      "bundler": "metro",
      "favicon": "@/assets/favicon.png"
    },
    "name": "move",
    "slug": "move",
    "extra": {
      "EXPO_PUBLIC_SUPABASE_URL": "https://jxjaqamunkkqnwhrlnzk.supabase.co",
      "EXPO_PUBLIC_SUPABASE_ANON_KEY": "sb_publishable_w7Zh42w1bNPIe3kicNVEEw_dBqEq_2I",
      "EXPO_PUBLIC_INGEST_URL": "https://jxjaqamunkkqnwhrlnzk.supabase.co/functions/v1/ingest",
      "EXPO_PUBLIC_API_BASE": "https://move-dashboard-deploy-production.up.railway.app",
      "router": {},
      "eas": {
        "projectId": "96370249-284e-417e-bb0e-4b1aacaaa305"
      }
    },
    "version": "1.0.0",
    "scheme": "move",
    "plugins": [
      "expo-router",
      "expo-camera",
      "@livekit/react-native-expo-plugin"
    ],
    "ios": {
      "bundleIdentifier": "com.naishadabla.move",
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We use the camera to scan event QR codes.",
        "NSMicrophoneUsageDescription": "We use the microphone for live audio during calls.",
        "NSMotionUsageDescription": "We use motion data to track your movement for scoring.",
        "UIBackgroundModes": [
          "audio",
          "audio"
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.naishadabla.move",
      "usesCleartextTraffic": true,
      "permissions": [
        "CAMERA",
        "ACTIVITY_RECOGNITION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "move"
            }
          ]
        }
      ]
    }
  }
}
